  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#ffffff80"
      }
    },
    {
      "name": "Text / General",
      "scope": [
        "text",
        "source punctuation",
        "meta.brace",
        "meta.array",
        "meta.structure.dictionary.json",
        "punctuation.separator.array",
        "punctuation.separator.key-value.html",
        "punctuation.terminator",
        "punctuation.separator",
        "punctuation.accessor",
        "text.html.markdown punctuation.definition"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "punctuation.definition.keyword",
        "entity.name.tag.html",
        "entity.name.tag.js.jsx",
        "entity.name.tag.tsx",
        "text.xml entity.name.tag"
      ],
      "settings": {
        "foreground": "#2e95cc"
      }
    },
    {
      "name": "Variable / Identifier",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Annotation",
      "scope": [
        "annotation",
        "support.variable.property",
        "storage.type.annotation"
      ],
      "settings": {
        "foreground": "#2e95cc"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "support.constant",
        "constant.character.numeric"
      ],
      "settings": {
        "foreground": "#d82f79"
      }
    },
    {
      "name": "Tag / Class",
      "scope": [
        "entity.name.tag",
        "entity.name.class",
        "support.class",
        "entity.other.inherited-class",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.xml",
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#d82f79"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.interpolated",
        "markup.inline.raw.string",
        "punctuation.definition.raw.markdown",
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown",
        "source.yaml string.quoted.double entity.name.tag", // TODO: MAKE CONTRIBUTION TO OTHER THEMES
        "markup.fenced_code string.quoted.double.yaml entity.name.tag.yaml", // TODO: MAKE CONTRIBUTION TO OTHER THEMES
      ],
      "settings": {
        "foreground": "#00a67d"
      }
    },
    {
      "name": "NUMBER",
      "scope": [
        "constant.numeric",
        "constant.integer",
        "constant.floating-point",
        "punctuation.separator.decimal.period.php",
        "punctuation.definition.list.begin.markdown",
        "keyword.other.unit",
      ],
      "settings": {
        "foreground": "#d82f79"
      }
    },
    {
      "name": "Function / Method",
      "scope": [
        "variable.function",
        "entity.name.function",
        "string.other.link",
        "support.type.vendored",
        "support.other.variable",
        "meta.function-call.generic",
        "support.function.builtin",
        "meta.method-call.groovy meta.method.groovy",
        "meta.class.body.groovy meta.method.body.java storage.type.groovy",
        "punctuation.definition.decorator",
        "entity.name.function.decorator",
        "support.function.any-method",
        "text.tex support.function",
        "text.tex punctuation.definition.function",
        "support.variable.class.function",
        "keyword.control.cucumber.table",
        "entity.name.function.decorator",
        "support.function.construct",
