  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#a0a1a7"
      }
    },
    {
      "name": "Text / General",
      "scope": [
        "text",
        "source punctuation",
        "meta.brace",
        "meta.array",
        "meta.structure.dictionary.json",
      ],
      "settings": {
        "foreground": "#383a42"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#a626a4"
      }
    },
    {
      "name": "Variable / Identifier",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#383a42"
      }
    },
    {
      "name": "Annotation",
      "scope": [
        "annotation",
        "support.variable.property",
        "storage.type.annotation"
      ],
      "settings": {
        "foreground": "#0184bb"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "support.constant",
        "constant.character.numeric"
