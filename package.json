{
  "name": "bedarx-pro",
  "displayName": "BedarX Pro",
  "description": "A ChatGPT-inspired theme with Light and Dark variants, offering exceptional readability and visual comfort.",
  "version": "0.0.1",
  "publisher": "saqibbedar",
  "author": {
    "name": "Saqib Bedar"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saqibbedar/BedarX-Pro"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "keywords": [
    "theme",
    "dark theme",
    "light theme",
    "bedarx",
    "bedarx pro",
    "ChatGPT theme",
    "high contrast",
    "color theme"
  ],
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "BedarX Pro Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-warm-color-theme.json"
      },
      {
        "label": "BedarX Pro Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-crimson-color-theme.json"
      },
      {
        "label": "BedarX Pro Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-color-theme.json"
      },
      {
        "label": "BedarX Pro Light",
        "uiTheme": "vs",
        "path": "./themes/light-color-theme.json"
      }
    ]
  },
  "galleryBanner": {
    "color": "#0d0d0d",
    "theme": "dark"
  },
  "icon": "Public/theme-icon.png",
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "sponsor": {
    "url": "https://buymeacoffee.com/saqibbedar"
  },
  "features": [
    "Dark and Light themes with carefully crafted color schemes",
    "ChatGPT-inspired design for enhanced readability",
    "Optimized syntax highlighting for 15+ programming languages",
    "High contrast ratios for better accessibility",
    "Custom workbench colors for improved UI experience",
    "Enhanced terminal and debug console colors",
    "Refined editor widget styling",
    "Git decoration colors for clear version control status",
    "Consistent bracket pair colorization",
    "Carefully selected semantic highlighting"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saqibbedar/BedarX-Pro/issues"
  },
  "homepage": "https://github.com/saqibbedar/BedarX-Pro",
  "badges": [
    {
      "url": "https://vsmarketplacebadges.dev/version/",
      "href": "https://marketplace.visualstudio.com/items/",
      "description": "Extension version"
    },
    {
      "url": "https://vsmarketplacebadges.dev/installs/",
      "href": "https://marketplace.visualstudio.com/items/",
      "description": "Extension installs"
    },
    {
      "url": "https://vsmarketplacebadges.dev/rating/",
      "href": "https://marketplace.visualstudio.com/items/",
      "description": "Extension Rating"
    }
  ],
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@vscode/vsce": "^2.32.0"
  }
}
